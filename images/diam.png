    // handle mouse click away from menu to close
    $(document).on('click', function (e) {
        if ($('.canva-button-menu').css('display') === 'none') return;

        const menu = document.querySelector('.canva-button-menu');

        if (menu === e.target) return;
        if (e.target.closest('.canva-button-menu')) return;
        if (e.target.classList.contains('canva-show-more')) return;

        $(menu).slideUp(100);
    });

    // handle escape key to close menu
    $(document).on('keyup', function (e) {
        if (e.key === 'Escape' && $('.canva-button-menu').css('display') !== 'none') {
            $('.canva-button-menu').slideUp(100);
        }
    });
